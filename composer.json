{
	"name": "jedkirby/portfolio",
	"description": "James Kirby's Portfolio.",
	"type": "project",
	"require": {
		"laravel/framework": "5.1.*",
		"illuminate/html": "5.*",
		"thujohn/twitter": "2.0.*@dev",
		"fzaninotto/faker": "1.4.0",
		"social-links/social-links": "1.5.*",
		"pulse00/monolog-parser": "^0.0.3",
		"guzzlehttp/guzzle": "^6.0",
		"guzzlehttp/oauth-subscriber": "^0.3.0"
	},
	"require-dev": {
		"phpunit/phpunit": "~4.0",
		"phpspec/phpspec": "~2.1"
	},
	"autoload": {
		"classmap": [
			"database"
		],
		"psr-4": {
			"App\\": "app/"
		},
		"files": [
			"app/Support/helpers.php"
		]
	},
	"autoload-dev": {
		"psr-4": {
			"Test\\App\\": "tests/"
		}
	},
	"config": {
		"preferred-install": "dist",
		"secure-http": false
	}
}
