{
	"name": "jedkirby/portfolio",
	"description": "James Kirby's Portfolio.",
	"type": "project",
	"require": {
		"php": "^5.6.22",
		"laravel/framework": "5.3.*",
		"social-links/social-links": "1.5.*",
		"pulse00/monolog-parser": "^0.0.3",
		"guzzlehttp/guzzle": "^6.0",
		"guzzlehttp/oauth-subscriber": "^0.3.0",
		"jedkirby/tweet-entity-linker": "^1.1",
		"laravelcollective/html": "^5.2.0"
	},
	"require-dev": {
		"phpunit/phpunit": "~4.0",
		"satooshi/php-coveralls": "^1.0",
		"mockery/mockery": "^0.9.7",
		"jedkirby/php-cs": "^1.0"
	},
	"autoload": {
		"classmap": [
			"database"
		],
		"psr-4": {
			"App\\": "app/"
		},
		"files": [
			"app/Domain/Support/helpers.php"
		]
	},
	"autoload-dev": {
		"psr-4": {
			"App\\Tests\\": "tests/"
		}
	},
	"scripts": {
		"compile": [
			"php artisan instagram:latest-posts",
			"php artisan twitter:latest-tweet"
		]
	},
	"config": {
		"platform": {
			"php": "5.6.22"
		}
	}
}
